<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "--//mybatis.org/DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.cozi.poothrow.mapper.TestMapper">
        <select id="testList2" resultType="com.cozi.poothrow.domain.TestVO">
                select * from TEST
        </select>

        <insert id="insertTest" parameterType="com.cozi.poothrow.domain.TestVO">
                insert into TEST(ID, NAME, email)
                VALUES(#{id}, #{name}, #{email})
        </insert>






</mapper>
